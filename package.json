{
  "name": "kysely-duckdb",
  "version": "0.2.0",
  "description": "kysely dialect for duckdb",
  "exports": {
    "require": "./dist/index.js",
    "import": "./dist/index.mjs",
    "types": "./dist/index.d.ts"
  },
  "scripts": {
    "test": "jest",
    "build": "rimraf ./dist && tsup src/index.ts --format cjs,esm --dts --sourcemap",
    "check": "tsc --noEmit",
    "docs": "typedoc src/index.ts",
    "all": "npm run check && npm run test && npm run build && npm run docs"
  },
  "keywords": [
    "kysely",
    "kysely-dialect",
    "duckdb"
  ],
  "author": "runoshun",
  "license": "MIT",
  "devDependencies": {
    "@duckdb/node-api": "^1.3.4-alpha.27",
    "@tsconfig/node18": "^18.2.2",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.7.0",
    "dprint": "^0.41.0",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "tsup": "^7.2.0",
    "typedoc": "^0.25.1",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "@duckdb/node-api": "*",
    "kysely": "*"
  }
}
